{
  "name": "ouchi-finance",
  "author": "giorgiPapava & dunkman32",
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "dev": "yarn i18n && next dev -p 3000",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,json,md,ts,tsx}\"",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "test": "jest",
    "check-all": "yarn format && yarn lint --fix && yarn type-check",
    "build": "yarn i18n && next build",
    "export": "yarn build && next export",
    "extract:i18n": "formatjs extract '{pages,components,sections}/**/*.{js,ts,tsx}' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "compile:i18n": "formatjs compile-folder --ast --format simple intl/locales intl/compiled-locales",
    "i18n": "yarn extract:i18n && yarn compile:i18n"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run check-all && git add .",
      "pre-push": "yarn run lint && yarn run type-check"
    }
  },
  "dependencies": {
    "@material-ui/core": "latest",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@material-ui/styles": "^4.11.3",
    "@reduxjs/toolkit": "^1.5.1",
    "axios": "^0.21.1",
    "clsx": "^1.1.1",
    "date-fns": "^2.21.3",
    "dom-to-image": "^2.6.0",
    "firebase": "^8.9.1",
    "formik": "^2.2.9",
    "lodash": "^4.17.21",
    "nanoid": "^3.1.23",
    "next": "latest",
    "next-images": "^1.7.0",
    "notistack": "^1.0.7",
    "qs": "^6.10.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-flow-renderer": "^9.6.3",
    "react-intl": "^5.20.9",
    "react-redux": "^7.2.3",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.2.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@formatjs/cli": "^4.2.23",
    "@formatjs/ts-transformer": "^3.4.3",
    "@testing-library/react": "^11.2.5",
    "@types/dom-to-image": "^2.6.3",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.170",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@types/reselect": "^2.2.0",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-formatjs": "^10.3.0",
    "babel-plugin-styled-components": "1.8.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.0.10",
    "prettier": "^2.0.2",
    "typescript": "^4.1.3"
  }
}
